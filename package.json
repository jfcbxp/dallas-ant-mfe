{
	"name": "dallas-ant-mfe",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"commit": "git-cz",
		"dev": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && next dev -p 3006",
		"build": "node envs.js && next build",
		"build:windows": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && node envs.js && next build",
		"format": "prettier --write \"src/**/*.ts\" ",
		"start": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && export NODE_ENV=production && next start -p 3006",
		"deploy": "bash deploy.bash",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:patch": "standard-version --release-as patch",
		"release:major": "standard-version --release-as major",
		"test": "jest ",
		"test:ci": "jest --ci",
		"prepare": "husky"
	},
	"dependencies": {
		"next": "16.1.3",
		"primeicons": "^7.0.0",
		"primereact": "^10.9.5",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-icons": "^5.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.5",
		"@commitlint/config-conventional": "^17.6.5",
		"@eslint/eslintrc": "^3.3.3",
		"@tanstack/react-query": "5.29.0",
		"@testing-library/jest-dom": "^6.1.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/node": "^20",
		"@types/react": "^19",
		"@types/react-dom": "^19",
		"@types/styled-components": "^5.1.34",
		"@typescript-eslint/eslint-plugin": "^8.53.0",
		"@typescript-eslint/parser": "^8.53.0",
		"babel-plugin-styled-components": "^2.1.4",
		"commitizen": "4.3.1",
		"cypress": "13.9.0",
		"cypress-mochawesome-reporter": "^3.8.2",
		"eslint": "9",
		"eslint-config-next": "16.1.3",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-react-hooks": "^7.0.1",
		"git-cz": "4.8.0",
		"husky": "^9.0.11",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"prettier": "^3.8.0",
		"styled-components": "6.1.13",
		"typescript": "^5"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/git-cz"
		}
	}
}
