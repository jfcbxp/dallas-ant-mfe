{
  "name": "dallas-ant-mfe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "dev": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && next dev -p 3006",
    "build": "export NEXT_PRIVATE_LOCAL_WEBPACK=true && node envs.js && next build",
    "build:windows": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && node envs.js && next build",
    "format": "prettier '**/*.{ts,tsx}' --write",
    "start": "SET NEXT_PRIVATE_LOCAL_WEBPACK=true && export NODE_ENV=production && next start -p 3006",
    "deploy": "bash deploy.bash",
    "lint": "next lint",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "test": "jest ",
    "test:ci": "jest --ci",
    "prepare": "husky"
  },
  "dependencies": {
    "next": "16.1.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "primereact": "^10.9.5",
    "primeicons": "^7.0.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",

    "styled-components": "6.1.13",
    "babel-plugin-styled-components": "^2.1.4",

    "typescript": "^5",

    "eslint": "^9",
    "eslint-config-next": "16.1.3",
    "eslint-config-prettier": "8.5.0",

    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.4.3",

    "cypress": "13.9.0",
    "cypress-mochawesome-reporter": "^3.8.2",

    "husky": "^9.0.11",
    "commitizen": "4.3.1",
    "git-cz": "4.8.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/git-cz"
    }
  }
}
